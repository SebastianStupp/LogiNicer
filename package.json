{
  "name": "loginicer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "client": "cd client && npm start",
    "heroku-prebuild": "cd client && npm ci --only=prod",
    "build": "cd client && npm run build && npm run build-storybook",
    "storybook": "cd client && npm run storybook",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" \"npm run storybook\"",
    "dev:server": "npm run build && cd .. && npm start",
    "server": "nodemon index.js",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SebastianStupp/LogiNicer.git"
  },
  "author": "Sebastian Stupp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SebastianStupp/LogiNicer/issues"
  },
  "homepage": "https://github.com/SebastianStupp/LogiNicer#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  },
  "dependencies": {
    "add": "^2.0.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "latest": "^0.2.0",
    "mongodb": "^3.5.5"
  },
  "proxy": "http://localhost:8080"
}
